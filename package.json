  "bugs": {
    "url": "https://github.com/aflover/vaf/issues"
  },
  "homepage": "https://github.com/aflover/vaf#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "rollup": "^0.36.3",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-watch": "^2.5.0",
    "uglifyjs": "^2.4.10"
  }
